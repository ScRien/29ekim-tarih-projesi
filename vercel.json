{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/videos/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000",
        "Content-Type": "video/mp4"
      },
      "dest": "/videos/$1"
    },
    {
      "src": "/images/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      },
      "dest": "/images/$1"
    },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
